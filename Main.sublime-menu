[
    {
        "id": "preferences",
        "children": [
            {
                "id": "package-settings",
                "caption": "Package Settings",
                "children": [
                    {
                        "caption": "Pretty Shell",
                        "children": [
                            {
                                "caption": "Settings",
                                "command": "edit_settings",
                                "args": {
                                    "base_file": "${packages}/Pretty Shell/Pretty Shell.sublime-settings",
                                    "default": {
                                        "indent": 4,
                                        "pretty_on_save": true
                                    }
                                }
                            },
                            {
                                "caption": "Key Bindings",
                                "command": "edit_settings",
                                "args": { "base_file": "${packages}/Pretty Shell/Default ($platform).sublime-keymap" }
                            }
                        ]
                    }
                ]
            }
        ]
    }
]